{
    "position": "top",
    "layer": "top",
    "height": 16,
    "margin-top": 0,
    "margin-bottom": 0,
    "margin-left": 0,
    "margin-right": 0,
    "modules-left": ["custom/launcher", "hyprland/workspaces", "custom/playerctl", "custom/playerlabel"],
    "modules-center": ["cpu", "memory", "disk"],
    "modules-right": ["tray", "custom/randwall", "network", "pulseaudio", "clock"],
    // Modules configuration
	"clock": {
		"format": "ÔÄó {:%H:%M}",
		"tooltip": "true",
        	"tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        	"format-alt": "ÔÅ≥ {:%d/%m}"
	},
	

	"sway/workspaces": {
         "active-only": false,
         "disable-scroll": true,
         "all-outputs": true,
         "warp-on-scroll": false,
         "on-scroll-up": "hyprctl dispatch workspace -1",
         "on-scroll-down": "hyprctl dispatch workspace -1",
         "format": "{name}: {icon}",
         "format-icons": {
             "1": "ÔÑ†",
             "2": "Ôâ®",
             "3": "ÔÑ°",
             "4": "ÔÜº",
             "5": "ÔÅµ",
             "urgent": "ÔÅ™",
             "focused": "ÔÜí",
             "default": "ÔÑë",
           "sort-by-number": true
       }
     },

    "custom/playerctl": {
      "format": "{icon}",
      "return-type": "json",
      "max-length": 64,
      "exec": "playerctl -a metadata --format '{\"text\": \"{{artist}} - {{markup_escape(title)}}\", \"tooltip\": \"{{playerName}} : {{markup_escape(title)}}\", \"alt\": \"{{status}}\", \"class\": \"{{status}}\"}' -F",
      "on-click-middle": "playerctl play-pause",
      "on-click": "playerctl previous",
      "on-click-right": "playerctl next",
      "format-icons": {
        "Playing": "<span foreground='#E5B9C6'>Û∞íÆ Û∞êå Û∞í≠</span>",
        "Paused": "<span foreground='#928374'>Û∞íÆ Û∞è• Û∞í≠</span>"
      },
    },

    "custom/playerlabel": {
      "format": "<span>{}</span>",
      "return-type": "json",
      "max-length": 48,
      "exec": "playerctl -a metadata --format '{\"text\": \"{{artist}} - {{markup_escape(title)}}\", \"tooltip\": \"{{playerName}} : {{markup_escape(title)}}\", \"alt\": \"{{status}}\", \"class\": \"{{status}}\"}' -F",
      "on-click-middle": "playerctl play-pause",
      "on-click": "playerctl previous",
      "on-click-right": "playerctl next",
      "format-icons": {
        "Playing": "<span foreground='#E5B9C6'>Û∞íÆ Û∞êå Û∞í≠</span>",
        "Paused": "<span foreground='#928374'>Û∞íÆ Û∞è• Û∞í≠</span>"
      },
    },

	"battery": {
        "states": {
            "good": 95,
            "warning": 30,
            "critical": 15
        },
        "format":"{icon}  {capacity}%",
        "format-charging": "{capacity}% Ôóß",
        "format-plugged": "{capacity}% Ôá¶",
        "format-alt": "{icon} {time}",
        // "format-good": "", // An empty format will hide the module
        // "format-full": "",
        "format-icons": ["ÔâÑ", "ÔâÉ", "ÔâÇ", "ÔâÅ", "ÔâÄ"]
	},

  "memory": {
    "format": "Û∞çõ {}%",
    "format-alt": "Û∞çõ {used}/{total} GiB",
    "interval": 5
  },

  "cpu": {
    "format": "Û∞ª† {usage}%",
    "format-alt": "Û∞ª† {avg_frequency} GHz",
    "interval": 5
  },

  "disk": {
    "format": "Û∞ãä {}%",
    "format-alt": "Û∞ãä {used}/{total} GiB",
    "interval": 5,
    "path": "/mnt/Datos"
  },
	
	"network": {
        	"format-wifi": "Û∞§®",
        	"format-ethernet": "Ôõø {ifname}: Aesthetic",
        	"format-linked": "Ôõø {ifname} (No IP)",
        	"format-disconnected": "Û∞§≠",
        	"format-alt": "Ôõø {ifname}: {ipaddr}/{cidr}",
          "tooltip-format": "{essid}",
          "on-click-right": "nm-connection-editor"
	},

	"tray": {
		"icon-size": 16,
		"spacing": 5
	},

	"backlight": {
        // "device": "acpi_video1",
	"format": "{icon} {percent}%",
        "format-icons": ["Óéç", "Óèì", "Óèë", "Óèè", "Óèé", "Óèç", "Óèä", "Óèà", "Óéõ"],
//	"on-scroll-up":,
//	"on-scroll-down":,
	},

	"pulseaudio": {
		"format": "{icon} {volume}%",
        	"format-muted": "Û∞ùü",
		"format-icons": {
			"default": ["Û∞ïø", "Û∞ñÄ", "Û∞ïæ"]
		},
		"on-click": "bash ~/.scripts/volume mute",
    "on-scroll-up": "bash ~/.scripts/volume up",
    "on-scroll-down": "bash ~/.scripts/volume down",
    "scroll-step": 5,
		"on-click-right": "pavucontrol"
  },
//   "custom/randwall": {
//     "format": "Û∞èò",
//     "on-click": "bash $HOME/.config/hypr/randwall.sh",
//     "on-click-right": "bash $HOME/.config/hypr/wall.sh"
//   },
  "custom/launcher": {
    "format": "Û∞à∏",
    "on-click": "bash $HOME/.config/rofi/launcher.sh",
//    "on-click": "wofi --show drun -I -a -n -W 500 -H 376 -s ~/.config/wofi/themes/gruvbox.css",
    "on-click-right": "bash $HOME/.config/rofi/run.sh"  
//    "on-click-right": "wofi --show run -I -a -n -W 500 -H 376 -s ~/.config/wofi/themes/gruvbox.css"  
  },

  "custom/wf-recorder": {
		"format": "{}",
		"interval": "once",
		"exec": "echo 'Ôäç'",
		"tooltip": "false",
		"exec-if": "pgrep 'wf-recorder'",
		"on-click": "exec ./scripts/wlrecord.sh",
		"signal": 8
	},

